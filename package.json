{
  "name": "traffic-dash",
  "productName": "traffic-dash",
  "version": "1.0.0",
  "description": "Traffic dashboard for New Zealand",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "npm-run-all --parallel serve-local start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "serve-local": "node ./src/local.js"
  },
  "keywords": [],
  "author": "Richard Law",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "traffic_dash"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "trafficdash"
      }
    }
  },
  "dependencies": {
    "aws-sdk": "^2.168.0",
    "axios": "^0.19.0",
    "clipboardy": "^1.2.2",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.42.0",
    "material-ui": "^1.0.0-beta.23",
    "material-ui-icons": "^1.0.0-beta.17",
    "mkdirp": "^0.5.1",
    "moment-timezone": "^0.5.35",
    "node-env-file": "^0.1.8",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-hot-loader": "^3.1.2",
    "react-live-clock": "^2.0.2",
    "react-mapbox-gl": "^2.7.1",
    "serve-static": "^1.13.1",
    "turf-helpers": "^3.0.12",
    "typeface-roboto": "0.0.45",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/runtime": "<=7.0.0-beta.55",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^4.1.3",
    "electron-prebuilt-compile": "1.7.9",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "npm-run-all": "^4.1.3"
  }
}
